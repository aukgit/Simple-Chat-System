<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="chatdatabase" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>EntityClass.User</class>
    <class>EntityClass.Serversetting</class>
    <class>EntityClass.Chatlist</class>
    <class>EntityClass.Chatsession</class>
    <class>EntityClass.Message</class>
    <class>TestingCode.SwingListExample</class>
    <class>EntityClass.Forms.Chatlist</class>
    <class>EntityClass.Userstatus</class>
    <class>EntityClass.Userrecentstatus</class>
    <class>EntityClass.Chatsessionrelateduser</class>
    <class>EntityClass.Activestate</class>
    <class>OnlineServers.PictureUploadServer</class>
    <class>OnlineServers.UserOnline</class>
    <class>Mailer.Gmail</class>
    <class>EntityClass.Messagerecent</class>
    <class>EntityClass.Msgfile</class>
    <class>EntityClass.Towhomaliaswhat</class>
    <class>EntityClass.Usermsgreceived</class>
    <class>EntityClass.Friendrequest</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://locahost:3306/chatdatabase"/>
      <property name="javax.persistence.jdbc.password" value=""/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="admin"/>
    </properties>
  </persistence-unit>
</persistence>
